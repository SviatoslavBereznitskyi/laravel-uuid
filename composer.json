{
  "name": "imcity-tech/laravel-uuid",
  "description": "[fork] The package provides UUID keys functionality for Eloquent models with one single trait.",
  "license": "MIT",
  "keywords": ["uuid", "eloquent", "laravel"],
  "homepage": "https://github.com/nevadskiy/laravel-uuid",
  "authors": [
    {
      "name": "Nevadskiy",
      "email": "nevadskiy@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0",
    "ramsey/uuid": "^3.7|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Nevadskiy\\Uuid\\": "src/"
    },
    "files": [
      "helpers/constants.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nevadskiy\\Uuid\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "fix": "php-cs-fixer fix"
  }
}
