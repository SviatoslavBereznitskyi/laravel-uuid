{
  "name": "nevadskiy/laravel-uuid",
  "description": "The package provides UUID keys functionality for Eloquent models.",
  "license": "MIT",
  "keywords": ["uuid", "laravel"],
  "homepage": "https://github.com/nevadskiy/laravel-uuid",
  "authors": [
    {
      "name": "Nevadskiy",
      "email": "nevadskiy@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "laravel/framework": "^6.0|^7.0|^8.0",
    "ramsey/uuid": "^3.7|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^5.0",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Nevadskiy\\Uuid\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nevadskiy\\Uuid\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "fix": "php-cs-fixer fix"
  }
}
